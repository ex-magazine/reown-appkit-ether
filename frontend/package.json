    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky",
    "schema1:generate": "prisma generate  --schema ./prisma/schema1.prisma",
    "schema2:generate": "prisma generate  --schema ./prisma/schema2.prisma",
